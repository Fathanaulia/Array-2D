#include <iostream>
#include <conio.h>
#include <windows.h>
using namespace std;

int main(){

     // Deklarasi dan inisialisasi array dua dimensi
     // Dengan jumlah elemen baris = 3
     // dan jumlah elemen kolom = 2
     int matrikcontoh [3][2] = {{4,5},{6,7},{8,9}};

     // Mendeklarasi variabel untuk
     // Indeks perulangan
     int x,z;

     cout<<"\t==============================";
     cout<<"\n\t=== Tampil Matrik Ordo 3x2 ===\n";
     cout<<"\t==============================\n\n";

     // Menampilkan matrikcontoh
     cout<<"\nMenampilkan matrik contoh\n";
     for(x=0;x<3;x++){
          for(z=0;z<2;z++){
               cout<<matrikcontoh[x][z]<<" ";
          }
          cout<<endl;
     }

     return 0;
}
